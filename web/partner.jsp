ps.setString(8, qq);ps.setString(9, tt);ps.setString(10, ee);ps.setString(11, ep);ps.setString(12, ei);ps.setString(13, fn);    
    ps.execute();
    c.close(); 
    out.println("<script>alert('record saved');</script>");
    
